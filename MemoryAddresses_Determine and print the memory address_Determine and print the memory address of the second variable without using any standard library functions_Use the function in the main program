#include <iostream>
using namespace std;

template <typename T1, typename T2>
void memoryAddresses(T1 var1, T2 var2) {
    cout << "Memory address of first variable (var1): " << &var1 << endl;

    cout << "Memory address of second variable (var2): " << &var2 << endl;
}

int main() {
    int intVar = 42;
    double doubleVar = 3.14;

    memoryAddresses(intVar, doubleVar);

    return 0;
}
